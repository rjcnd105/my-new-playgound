{
  "name": "my-new-playground",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "configs/*",
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "clean": "concurrently \"pnpm run clean:root\" \"pnpm run clean:workspaces\"",
    "clean:root": "rm -rf ./node_modules",
    "clean:workspaces": "turbo run clean",
    "build:eslint": "turbo run build --filter=@hj/eslint-config",
    "storybook:lib": "turbo run storybook --filter=@hj/react-lib",
    "storybook:playground": "turbo run storybook --filter=playground-nextjs",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@effect/data": "^0.18.7",
    "@effect/io": "^0.41.2",
    "@effect/match": "^0.40.0",
    "@effect/query": "^0.7.0",
    "@effect/rpc": "^0.11.1",
    "@effect/schema": "^0.42.0",
    "@effect/stm": "^0.25.0",
    "@effect/stream": "^0.37.1",
    "@effect/typeclass": "^0.9.0",
    "axios": "^1.5.1",
    "effect": "^2.0.0-next.47",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hj/eslint-config": "workspace:*",
    "@manypkg/cli": "^0.21.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.51.0",
    "tsup": "^7.2.0",
    "turbo": "^1.10.15",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}