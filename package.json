{
  "name": "my-new-playground",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*",
    "examples/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "clean": "concurrently \"pnpm run clean:root\" \"pnpm run clean:workspaces\"",
    "clean:root": "rm -rf ./node_modules",
    "clean:workspaces": "turbo run clean",
    "build:eslint": "turbo run build --filter=@hj/eslint-config",
    "storybook:lib": "turbo run storybook --filter=@hj/react-lib",
    "storybook:playground": "turbo run storybook --filter=playground-nextjs",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@effect/schema": "^0.68.0",
    "@tanstack/react-query": "^5.45.1",
    "effect": "^3.5.4",
    "option-t": "^49.1.0",
    "react": "next",
    "react-dom": "next",
    "remeda": "^2.2.0",
    "typescript-eslint": "rc-v8",
    "valibot": "^0.34.0",
    "zod": "^3.23.8",
    "@visx/axis": "^3.10.1",
    "@visx/bounds": "^3.3.0",
    "@visx/curve": "^3.3.0",
    "@visx/event": "^3.3.0",
    "@visx/group": "^3.3.0",
    "@visx/legend": "^3.10.3",
    "@visx/point": "^3.3.0",
    "@visx/scale": "^3.5.0",
    "@visx/shape": "^3.5.0",
    "@visx/text": "^3.3.0",
    "@visx/tooltip": "^3.3.0",
    "@visx/wordcloud": "^3.3.0",
    "axios": "1.7.2",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "d3-array": "^3.2.4",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.2.10",
    "lexical": "^0.16.0"
  },
  "devDependencies": {
    "@effect/typeclass": "^0.24.30",
    "@eslint/js": "^9.5.0",
    "@pandacss/dev": "^0.41.0",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "@types/d3": "^7.4.3",
    "@types/d3-array": "^3.2.1",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "tsx": "^4.15.6",
    "postcss": "^8.4.38",
    "postcss-import": "^15.1.0",
    "esbuild": "^0.21.5",
    "lightningcss": "^1.25.1",
    
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.5.14",
    "turbo": "^2.0.4",
    "type-fest": "^4.20.1",
    "typescript": "^5.5.1-rc",
    "tailwindcss": "^3.4.4",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "^4.3.2"
  }
}