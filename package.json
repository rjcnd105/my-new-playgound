{
  "name": "my-new-playground",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": ["packages/*", "apps/*", "examples/*"],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "clean": "concurrently \"pnpm run clean:root\" \"pnpm run clean:workspaces\"",
    "clean:root": "rm -rf ./node_modules",
    "clean:workspaces": "turbo run clean",
    "storybook:lib": "turbo run storybook --filter=@hj/react-lib",
    "storybook:playground": "turbo run storybook --filter=playground-nextjs",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@effect/schema": "^0.75.2",
    "@tanstack/react-query": "^5.59.3",
    "effect": "^3.9.1",
    "option-t": "^49.2.1",
    "neverthrow": "^8.0.0",
    "react": "rc",
    "react-dom": "rc",
    "remeda": "^2.15.0",
    "valibot": "^0.42.1",
    "zod": "^3.23.8",
    "@visx/axis": "^3.10.1",
    "@visx/bounds": "^3.3.0",
    "@visx/curve": "^3.3.0",
    "@visx/event": "^3.3.0",
    "@visx/group": "^3.3.0",
    "@visx/legend": "^3.10.3",
    "@visx/point": "^3.3.0",
    "@visx/scale": "^3.5.0",
    "@visx/shape": "^3.5.0",
    "@visx/text": "^3.3.0",
    "@visx/tooltip": "^3.3.0",
    "@visx/wordcloud": "^3.3.0",
    "vaul": "^1.0.0",
    "axios": "1.7.7",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "d3-array": "^3.2.4",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.11.4",
    "react-call": "^1.3.0",
    "@legendapp/state": "3.0.0-beta.8",
    "lexical": "^0.18.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@effect/typeclass": "^0.28.1",
    "@pandacss/dev": "^0.46.1",
    "@types/d3": "^7.4.3",
    "@types/d3-array": "^3.2.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^9.0.1",
    "esbuild": "^0.24.0",
    "lightningcss": "^1.27.0",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^6.2.0",
    "postcss-preset-env": "^10.0.6",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
